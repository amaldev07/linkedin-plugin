{
  "manifest_version": 3,
  "name": "Linkedin DM",
  "version": "1.0",
  "description": "Click a button on the page automatically",
  "permissions": [
    "activeTab",
    "scripting"
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "web_accessible_resources": [
    {
      "resources": [
        "resume/resume.pdf"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ]
}